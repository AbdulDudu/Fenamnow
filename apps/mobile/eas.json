{
  "cli": {
    "version": ">= 5.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Debug",
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "simulator"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "cocoapods": "1.15.2",
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "beta"
      },
      "channel": "preview"
    },
    "production": {
      "bun": "1.0.15",
      "env": {
        "APP_VARIANT": "production"
      },
      "distribution": "store",
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {}
}
